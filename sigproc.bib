journal = {Communications of the ACM},
keywords = {3,4,56,57,9,and phrases,cipher,cr categories,cryptanalysis,cryptography,data encryption,des,encipher,encrypt,encryption,multiple encryption,standard},
mendeley-groups = {Thesis' Project Related Work/4. Combining encryption algorithms/4.3 Multiple,Thesis' Project Related Work/3. Vulnerabilities in criptographic mechanisms},
number = {7},
pages = {465--467},
title = {{On the security of multiple encryption}},
volume = {24},
year = {1981}
}

