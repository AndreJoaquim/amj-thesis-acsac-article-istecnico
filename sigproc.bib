keywords = {AES,bicliques,block ciphers,key recovery,preimage},
mendeley-groups = {Thesis' Project Related Work/3. Vulnerabilities in criptographic mechanisms},
pages = {344--371},
title = {Biclique cryptanalysis of the full {AES}},
